    <div class="container-fluid" style="margin-top: 4.5rem;">
        <div class="row" style="margin: 0; padding: 0;">
            <div class="col-3">
                <div class="card" style="max-width: 100%;">
                    <div class="card-body">
                        <h5 class="card-title">Service type</h5>
                        <select class="form-control form-control-lg">
